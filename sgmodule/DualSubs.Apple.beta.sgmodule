[Script]
DualSubs.TV.HLS.Main.m3u8 = type=http-response, pattern=^https?:\/\/(play|play-edge|hls)\.itunes\.apple\.com\/WebObjects\/(MZPlay|MZPlayLocal)\.woa\/hls\/playlist\.m3u8(\?.+)?$, requires-body=1, timeout=30, debug=1, script-path=https://raw.githubusercontent.com/DualSubs/DualSubs/beta/js/DualSubs.HLS.Main.m3u8.beta.js
DualSubs.TV.HLS.WebVTT.m3u8 = type=http-response, pattern=^https?:\/\/vod-(.+)-amt\.tv\.apple\.com\/itunes-assets\/(.+)_subtitles\.m3u8(\?.*dualsubs=\w+)$, requires-body=1, timeout=30, debug=1, script-path=https://raw.githubusercontent.com/DualSubs/DualSubs/beta/js/DualSubs.HLS.WebVTT.m3u8.beta.js
DualSubs.TV.SUB.WebVTT = type=http-response, pattern=^https?:\/\/vod-(.+)-amt\.tv\.apple\.com\/itunes-assets\/(.+)\.webvtt(\?.+)?$, requires-body=1, timeout=30, debug=1, script-path=https://raw.githubusercontent.com/DualSubs/DualSubs/beta/js/DualSubs.SUB.WebVTT.beta.js

DualSubs.TV+.HLS.Main.m3u8 = type=http-response, pattern=^https?:\/\/(play|play-edge|hls)\.itunes\.apple\.com\/WebObjects\/(MZPlay|MZPlayLocal)\.woa\/hls\/subscription\/playlist\.m3u8(\?.+)?$, requires-body=1, timeout=30, debug=1, script-path=https://raw.githubusercontent.com/DualSubs/DualSubs/beta/js/DualSubs.HLS.Main.m3u8.beta.js
DualSubs.TV+.HLS.WebVTT.m3u8 = type=http-response, pattern=^https?:\/\/(play|play-edge)\.itunes\.apple\.com\/WebObjects\/(MZPlay|MZPlayLocal)\.woa\/hls\/subscription\/stream\/playlist\.m3u8(\?.*dualsubs=\w+)$, requires-body=1, timeout=30, debug=1, script-path=https://raw.githubusercontent.com/DualSubs/DualSubs/beta/js/DualSubs.HLS.WebVTT.m3u8.beta.js
DualSubs.TV+.SUB.WebVTT = type=http-response, pattern=^https?:\/\/vod-(.+)-aoc\.tv\.apple\.com\/itunes-assets\/(.+)\.webvtt(\?.+)?$, requires-body=1, timeout=30, debug=1, script-path=https://raw.githubusercontent.com/DualSubs/DualSubs/beta/js/DualSubs.SUB.WebVTT.beta.js

DualSubs.Fitness.HLS.Main.m3u8 = type=http-response, pattern=^https?:\/\/(play|play-edge|hls)\.itunes\.apple\.com\/WebObjects\/(MZPlay|MZPlayLocal)\.woa\/hls\/workout\/playlist\.m3u8(\?.+)?$, requires-body=1, timeout=30, debug=1, script-path=https://raw.githubusercontent.com/DualSubs/DualSubs/beta/js/DualSubs.HLS.Main.m3u8.beta.js
DualSubs.Fitness.HLS.WebVTT.m3u8 = type=http-response, pattern=^https?:\/\/(hls|hls-svod)\.itunes\.apple\.com\/itunes-assets\/(.+)\.m3u8(\?.*dualsubs=\w+)$, requires-body=1, timeout=30, debug=1, script-path=https://raw.githubusercontent.com/DualSubs/DualSubs/beta/js/DualSubs.HLS.WebVTT.m3u8.beta.js
DualSubs.Fitness.SUB.WebVTT = type=http-response, pattern=^https?:\/\/(hls|hls-svod)\.itunes\.apple\.com\/itunes-assets\/(.+)\.webvtt(\?.+)?$, requires-body=1, timeout=30, debug=1, script-path=https://raw.githubusercontent.com/DualSubs/DualSubs/beta/js/DualSubs.SUB.WebVTT.beta.js

[MITM]
hostname = %APPEND% hls.itunes.apple.com, hls-svod.itunes.apple.com, vod-*.tv.apple.com
tcp-connection = true
